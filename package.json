{
  "name": "olymp-react",
  "version": "2.1.5",
  "private": true,
  "homepage": "https://olymp.ksrt12.ru/",
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "js-sha256": "^0.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "eslint:lint": "eslint --ext .ts,.tsx,.js --ignore-path .gitignore .",
    "eslint:fix": "yarn eslint:lint --fix",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build -m \"$(git show -s --format='Update from %h: %s')\"",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "update": "npx npm-check-updates -u && npm install",
    "server": "serve -s build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@itmo/eslint-config": "^0.1.4",
    "eslint": "^8.45.0",
    "gh-pages": "^4.0.0",
    "sass": "^1.55.0"
  }
}
